  "scripts": {
    "clean": "rm -rf ../assets/static/js/*",
    "update": "npm outdated > update",
    "build-webpack": "node_modules/.bin/webpack -p",
    "webpack-json": "cross-env NODE_ENV='development' node_modules/.bin/webpack --json > stats.json",
    "analyze-webpack": "webpack-bundle-size-analyzer stats.json",
    "test": "node scripts/checks/json-config.js",
    "browser-test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "webpack-server": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot",
    "pre-prod-server": "cross-env NODE_ENV='pre-production' DEBUG='false' NODE_PATH='.' PORT='3000' node server.js",
    "prod-server": "cross-env NODE_ENV='production' DEBUG='true' NODE_PATH='.' PORT='3000' nodemon server.js",
    "dev-server": "cross-env NODE_ENV='development' DEBUG='true' NODE_PATH='.' PORT='3000' nodemon server.js",
    "dev": "npm run clean && cross-env NODE_ENV='development' npm run build-webpack && npm run dev-server",
    "local-server": "cross-env NODE_ENV='development' LOCAL='true' DEBUG='true' NODE_PATH='.' PORT='3000' nodemon server.js",
    "local": "npm run clean && cross-env NODE_ENV='development' npm run build-webpack && npm run local-server"
